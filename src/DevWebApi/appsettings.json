{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "RnCore": {
    "Metrics": {
      "Enabled": true,
      "ApplicationName": "DevWebApp",
      "MeasurementTemplate": "{app}/{mode}/{type}",
      "DevelopmentMode": true,
      "DevelopmentModeValue": "dev",
      "ProductionModeValue": "production",
      "CsvFile": {
        "Enabled": true,
        "OutputDir": "./csv"
      }
    }
  }
}
